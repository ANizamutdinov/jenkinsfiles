def functions

pipeline {
    agent { node 'master' }
    stages {
        stage ('init') {
            steps {
                script {
                    functions = load "functions/general_functions.groovy"
                }
            }
        }
        stage ('Application build') {
            steps{
                script{
                    sv_build = functions.getSas("account", "blob")
                    println(sv_build)
                }
            }
        }
    }
    post {
        always {
            cleanWs()
        }
    }
}
